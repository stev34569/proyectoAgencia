                        <div class="icon-sm border-radius-md text-center me-2 d-flex align-items-center justify-content-center">
                            <span class="nav-link-text ms-1"><i class="fa-solid fa-cart-shopping"></i></span>
                        </div>
                        
                        <span class="nav-link-text ms-1">Pagar Carrito</span>
                    </a>
                </li>
               
               

                @if (Context.Session.GetString("RolUsuario").ToString() == "2")
                    {
                <li class="nav-item">
                    <a class="nav-link " href="@Url.Action("Contacto", "Contactos")">
                        <div class="icon-sm border-radius-md text-center me-2 d-flex align-items-center justify-content-center">
                            <span class="nav-link-text ms-1"><i class="fa-solid fa-envelopes-bulk"></i></span>
                        </div>

                        <span class="nav-link-text ms-1">Contactenos</span>
                    </a>
                </li>
                }

                 @if (Context.Session.GetString("RolUsuario").ToString() == "1")
                    {
                    <li class="nav-item">
                        <a class="nav-link " href="@Url.Action("ConsultarContactos", "Contactos")">
